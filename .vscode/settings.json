{
    "files.associations": {
        // Use "Jekyll (HTML)" so that Jekyll Liquid syntax is picked up correctly for validation and highlighting.
        "*.html": "jekyll",
        "*.md": "jekyll",
        // Revert the above rule for doc files.
        "README.md": "markdown",
        "development.md": "markdown",
        "setup_and_run.md": "markdown",
    },
}
